{
  "platform": {
    "apiRuntime": "node:20"
  },
  "routes": [
    {
      "route": "/upload-file",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "mimeTypes": {
    ".json": "application/json"
  },
  "defaultDocument": "index.html"
}
